{
  "name": "vanilla-framework-react",
  "version": "0.1.3",
  "description": "Vanilla Framework, built using React",
  "main": "build/index.js",
  "scripts": {
    "build": "node scripts/build.js",
    "build-storybook": "build-storybook",
    "clean": "rm -rf node_modules && rm -rf build",
    "lint": "eslint ./src/",
    "serve": "start-storybook -p ${PORT:-8102}",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "test": "yarn jest --watch --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanilla-framework/vanilla-framework-react.git"
  },
  "bugs": {
    "url": "https://github.com/vanilla-framework/vanilla-framework-react/issues"
  },
  "homepage": "https://github.com/vanilla-framework/vanilla-framework-react#readme",
  "license": "MIT",
  "dependencies": {
    "dedent-js": "^1.0.1",
    "natural-compare-lite": "^1.4.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@percy-io/percy-storybook": "^1.2.5",
    "@storybook/addon-info": "^3.2.14",
    "@storybook/addon-knobs": "^3.2.17",
    "@storybook/addon-options": "^3.2.17",
    "@storybook/react": "^3.2.14",
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "4.0.2",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "react": "^16.1.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.0",
    "react-error-overlay": "^3.0.0",
    "react-test-renderer": "^16.0.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "react": "^16.x",
    "react-dom": "^16.x",
    "vanilla-framework": "^1.6.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
